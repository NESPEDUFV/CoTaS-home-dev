BASE         <http://nesped1.caf.ufv.br/od4cot>
PREFIX cot:  <#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd:  <http://www.w3.org/2001/XMLSchema#>
PREFIX owl:  <http://www.w3.org/2002/07/owl#>
PREFIX mads: <http://www.loc.gov/mads/rdf/v1#>


</context> a owl:Ontology ;
    rdfs:comment "OD4CoT applications and application domains module." ;
    owl:imports </definition> ;
    .


cot:Component a owl:Class ;
    rdfs:comment "A part that composes the whole of an object." ;
    rdfs:subClassOf cot:Context ;
    .

cot:contains a owl:ObjectProperty ;
    rdfs:comment "Indicates that an object has another object as one of its components." ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:Component ;
    .

cot:component a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates which object is the contained component of another." ;
    rdfs:subPropertyOf cot:hasSpec ;
    rdfs:domain cot:Component ;
    .


cot:Connection a owl:Class ;
    rdfs:comment "A physical or digital connection between objects." ;
    rdfs:subClassOf cot:Context ;
    .

cot:connects a owl:ObjectProperty ;
    rdfs:comment "Indicates a possible connection between objects." ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:Connection ;
    .

cot:to a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the object to which the possible connection refers." ;
    rdfs:subPropertyOf cot:hasSpec ;
    rdfs:domain cot:Connection ;
    .

cot:through a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the medium through which a connection can happen." ;
    rdfs:subPropertyOf cot:hasSpec ;
    rdfs:domain cot:Connection ;
    rdfs:range cot:ConnectionInterface ;
    .

cot:value a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates a specific value assigned to a context." ;
    rdfs:domain cot:Context ;
    .

cot:oneOf a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates a list of specific values that can be assigned to a context." ;
    rdfs:domain cot:Context ;
    rdfs:range rdf:List ;
    .

cot:minInclusive a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates an inclusive minimum value that can be assigned to a context." ;
    rdfs:seeAlso xsd:minInclusive ;
    rdfs:domain cot:Context ;
    .

cot:maxInclusive a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates an inclusive maximum value that can be assigned to a context." ;
    rdfs:seeAlso xsd:maxInclusive ;
    rdfs:domain cot:Context ;
    .

cot:minExclusive a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates an exclusive minimum value that can be assigned to a context." ;
    rdfs:seeAlso xsd:minExclusive ;
    rdfs:domain cot:Context ;
    .

cot:maxExclusive a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates an exclusive maximum value that can be assigned to a context." ;
    rdfs:seeAlso xsd:maxExclusive ;
    rdfs:domain cot:Context ;
    .

cot:cardinality a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the exact number of times a context repeats." ;
    rdfs:seeAlso owl:cardinality ;
    rdfs:domain cot:Context ;
    rdfs:range xsd:nonNegativeInteger ;
    .

cot:minCardinality a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the minimum number of times a context repeats." ;
    rdfs:seeAlso owl:minCardinality ;
    rdfs:domain cot:Context ;
    rdfs:range xsd:nonNegativeInteger ;
    .

cot:maxCardinality a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the maximum number of times a context repeats." ;
    rdfs:seeAlso owl:maxCardinality ;
    rdfs:domain cot:Context ;
    rdfs:range xsd:nonNegativeInteger ;
    .

cot:DynamicContextCondition a owl:Class ;
    rdfs:comment "A condition under which a dynamic context assumes a certain value or range of values." ;
    .

cot:case a owl:ObjectProperty ;
    rdfs:comment "Indicates a condition under which a dynamic context assumes a certain value or range of values." ;
    rdfs:domain cot:DynamicContext ;
    rdfs:range cot:DynamicContextCondition ; 
    .

cot:else a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the default value the dynamic context will assume if no condition applies." ;
    rdfs:domain cot:DynamicContext ;
    rdfs:range cot:Context ;
    .

cot:when a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the specific condition under which the dynamic context will assume a value." ;
    rdfs:domain cot:DynamicContextCondition ;
    rdfs:range cot:Context ;
    .

cot:then a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:comment "Indicates the value assumed by the dynamic context if the condition is met." ;
    rdfs:domain cot:DynamicContextCondition ;
    rdfs:range cot:Context ;
    .

# Shape

cot:Shape a owl:Class, cot:ContextCategory ;
    rdfs:comment "The approximate shape and size of an object." ;
    rdfs:subClassOf cot:Context ;
    .

cot:shape a owl:ObjectProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:Shape ;
    .

cot:RectBox a owl:Class ;
    rdfs:comment "The shape of an object as a rectangular box." ;
    rdfs:subClassOf cot:Shape ;
    owl:hasKey ( cot:width cot:height cot:length ) ;
    .

cot:Cylinder a owl:Class ;
    rdfs:comment "The shape of an object as a cylinder." ;
    rdfs:subClassOf cot:Shape ;
    owl:hasKey ( cot:radius cot:height ) ;
    .

cot:CylindricalShell a owl:Class ;
    rdfs:comment "Represents the shape of an object as a cylindrical shell." ;
    rdfs:subClassOf cot:Shape ;
    owl:hasKey ( cot:radius cot:height cot:thickness ) ;
    .

cot:ShapeParameter a owl:Class ; rdfs:subClassOf cot:Context .
cot:Width a owl:Class ; rdfs:subClassOf cot:ShapeParameter .
cot:Height a owl:Class ; rdfs:subClassOf cot:ShapeParameter .
cot:Length a owl:Class ; rdfs:subClassOf cot:ShapeParameter .
cot:Radius a owl:Class ; rdfs:subClassOf cot:ShapeParameter .
cot:Thickness a owl:Class ; rdfs:subClassOf cot:ShapeParameter .
cot:Circumference a owl:Class ; rdfs:subClassOf cot:ShapeParameter .

cot:width a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Width ;
    .

cot:height a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Height ;
    .

cot:length a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Length ;
    .

cot:radius a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Radius ;
    .

cot:thickness a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Thickness ;
    .

cot:thickness a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Thickness ;
    .

cot:circumference a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:Shape ;
    rdfs:range cot:Circumference ;
    .

# Storage

cot:Storage a owl:Class ;
    rdfs:comment "The capacity of an object to store information or other things." ;
    rdfs:subClassOf cot:Context ;
    .

cot:PhysicalStorage a owl:Class, cot:ContextCategory ;
    rdfs:comment "The capacity of an object to physically store other things inside itself." ;
    rdfs:subClassOf cot:Storage ;
    .

cot:physicalStorage a owl:ObjectProperty ;
    rdfs:comment "Indicates that a smart object is capable of physically storing other things inside itself. " ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:PhysicalStorage ;
    .

cot:ItemStorage a owl:Class ;
    rdfs:comment "The capacity of an object to store and keep track of discrete items." ;
    rdfs:subClassOf cot:PhysicalStorage ;
    .

## Energy storage

cot:EnergyStorage a owl:Class, cot:ContextCategory ;
    rdfs:comment "Represents the capacity of an object to store energy." ;
    rdfs:subClassOf cot:Storage ;
    .

cot:energyStorage a owl:ObjectProperty ;
    rdfs:comment "Indicates that a object is capable of storing energy." ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:EnergyStorage ;
    .

## Data storage

cot:DataStorage a owl:Class, cot:ContextCategory ;
    rdfs:subClassOf cot:Storage ;
    rdfs:comment "The capacity of an object to store digital information." ;
    .

cot:dataStorage a owl:ObjectProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:DataStorage ;
    .

cot:AudioRecordStorage a owl:Class ;
    rdfs:subClassOf cot:DataStorage ;
    rdfs:comment "The capacity of an object to store audio recordings." ;
    .

cot:CredentialsStorage a owl:Class ;
    rdfs:subClassOf cot:DataStorage ;
    rdfs:comment "The capacity of an object to store credentials." ;
    .

# Power supply

cot:PowerSupply a owl:Class, cot:ContextCategory ;
    rdfs:comment "The means through which electric power is supplied to a device." ;
    rdfs:subClassOf cot:Context ;
    .

cot:powerSupply a owl:ObjectProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:PowerSupply ;
    .

## USB Supply

cot:UsbSupply a owl:Class ;
    rdfs:subClassOf cot:PowerSupply ;
    .

cot:UsbCSupply a owl:Class ;
    rdfs:subClassOf cot:UsbSupply ;
    .

cot:MicroUsbSuply a owl:Class ;
    rdfs:subClassOf cot:UsbSupply ;
    .

## Battery Supply

cot:BatterySupply a owl:Class ;
    rdfs:comment "The usage of battery cell(s) to supply electric power to a device." ;
    rdfs:subClassOf cot:PowerSupply ;
    .

cot:BatteryCell a owl:Class ;
    rdfs:comment "A device that generates electrical energy through chemical reactions." ;
    rdfs:subClassOf cot:Resource ;
    .

cot:batteryCell a owl:ObjectProperty ;
    rdfs:subClassOf cot:hasSpec ;
    rdfs:domain cot:BatterySupply ;
    rdfs:range cot:BatteryCell ;
    .

cot:BatteryChargingTime a owl:Class ;
    rdfs:comment "The average time it takes to fully charge a battery." ;
    rdfs:subClassOf cot:Context ;
    .

cot:chargingTime a owl:ObjectProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:range cot:BatteryChargingTime ;
    .

cot:AaBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:AaaBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:DBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:LiIonBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:LiPoBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:ReplaceableBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:UnreplaceableBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .
cot:RechargeableBattery a owl:Class ; rdfs:subClassOf cot:BatteryCell .


## AC power supply

cot:DcPowerSupply a owl:Class ;
    rdfs:comment "A direct current (DC) power supply." ;
    rdfs:subClassOf cot:PowerSupply ;
    .

cot:AcPowerSupply a owl:Class ;
    rdfs:comment "An alternated current (AC) power supply." ;
    rdfs:subClassOf cot:PowerSupply ;
    .

cot:Voltage a owl:Class ;
    rdfs:subClassOf cot:Context ;
    .

cot:EnergyPower a owl:Class ;
    rdfs:subClassOf cot:Context ;
    .

cot:acInputPower a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:AcPowerSupply ;
    rdfs:range cot:EnergyPower ;
    .

cot:cableLength a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:AcPowerSupply ;
    rdfs:range cot:length ;
    .

cot:acInputVoltage a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:AcPowerSupply ;
    rdfs:range cot:Voltage ;
    .

cot:acInputAmpere a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:AcPowerSupply ;
    rdfs:range cot:Voltage ;
    .

cot:acOutputVoltage a owl:ObjectProperty, owl:FunctionalProperty ;
    rdfs:subPropertyOf cot:hasContext ;
    rdfs:domain cot:AcPowerSupply ;
    rdfs:range cot:Voltage ;
    .

# Color

cot:Color a owl:Class, cot:ContextCategory ;
    rdfs:comment "The visual perception presented by an object based on the frequency of incident light." ;
    rdfs:subClassOf cot:Context ;
    .

cot:color a owl:ObjectProperty ;
    rdfs:subClassOf cot:hasContext ;
    rdfs:range cot:Color ;
    .

